beo_ont:fossielaard
    a               owl:Ontology ;
    rdfs:comment
                    "Specifieert hoe de data in de codelijst van fossielaard te encoderen in RDF (i.e. RDF termen, kardinaliteiten,...)."@en ;
    rdfs:label      "fossielaard AP Constraints Limited"@en ;
    dct:abstract    "Het fossielaard applicatieprofiel specifieert de RDF termen (klassen en properties) en bijhorende constraints die gebruikt worden voor het beschrijven van fossielaard. Het maakt hierbij zoveel mogelijk gebruik van bestaande termen (klassen en properties) uit bestaande , skos, dbo, dbp en een minimale set van nieuwe termen gedefinieerd in het fossielaard Vocabularium."@en ;
    dct:alternative "fossielaard AP Shapes"@en ;
    dct:creator     beo_ag_cl:creator ;
    dct:description "Dit is de SHACL encodering van het applicatieprofiel van een fossielaard dat de eerste stap van de transformatie vanuit csv controleert."@en ;
    dct:issued      "2023-10-23"^^xsd:date ;
    dct:title       "fossielaard AP Constraints Limited"@en ;
    xkos:belongsTo  beo_dataset:codelijst-fossielaard ;
    xhv:last        beo_voc:fossielaard ;
    owl:sameAs      beo_voc:fossielaard ;
    owl:versionInfo "Versie 3.0.0  maart 2024"@en .


beo_voc:fossielaard
    a          owl:Ontology ;
    rdfs:label "fossielaard AP Constraints"@en ;
    owl:sameAs beo_ont:fossielaard .

beo_ag_cl:creator
    rdfs:label "Ontwikkelaar codelijst"@en ;
    schema:affiliation vl_org:OVO003749 ;
    a foaf:Person;
    foaf:firstName "Geert";
    foaf:lastName "Van Haute";
    foaf:mbox <mailto:geert.vanhaute@vlaanderen.be>.

# Node Shapes
beo_ns:schema
    a                sh:NodeShape ;
    rdfs:label       "conceptschema van fossielaard"@en ;
    sh:name          "conceptschema van fossielaard"@en ;
    sh:description   "Het schema waarbinnen de fossielaard gedefinieerd zijn"@en ;
    rdfs:isDefinedBy beo_ont:fossielaard, beo_voc:fossielaard ;
    sh:property      beo_ps:conceptscheme_prefLabel, beo_ps:schema_hasTopConcept, beo_ps:dc_identifier, beo_ps:notation, beo_ps:note,
                     beo_ps:definition, beo_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:ConceptScheme .

beo_ns:collection
    a                sh:NodeShape ;
    rdfs:label       "verzameling"@en ;
    sh:name          "verzameling"@en ;
    sh:description   "Verzamelingen bundelen fossielaard met gelijkaardige eigenschappen."@en ;
    rdfs:isDefinedBy beo_ont:fossielaard, beo_voc:fossielaard ;
    sh:property      beo_ps:prefLabel, beo_ps:collection_member, beo_ps:dc_identifier, beo_ps:notation,
                     beo_ps:inScheme, beo_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:Collection .

beo_ns:concept
    a                sh:NodeShape ;
    rdfs:label       "matrix"@en ;
    sh:name          "matrix"@en ;
    sh:description   "analysemethodes"@en ;
    rdfs:comment     "compendia onderhouden door VITO"@en ;
    rdfs:isDefinedBy beo_ont:fossielaard, beo_voc:fossielaard ;
    sh:property      beo_ps:notation, beo_ps:inScheme,
                     beo_ps:references, beo_ps:definition,
                     beo_ps:prefLabel, beo_ps:theme,
                     beo_ps:topConceptOf, beo_ps:altLabel, beo_ps:dc_identifier,
                     beo_ps:rdf_type, beo_ps:note, beo_ps:broaderTransitive, beo_ps:broader,
                     beo_ps:narrowerTransitive, beo_ps:narrower, beo_ps:semanticRelation ,
                     beo_ps:seeAlso;
    sh:closed true ;
    sh:targetClass   skos:Concept .

